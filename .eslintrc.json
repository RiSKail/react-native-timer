{
    "extends": [
      "airbnb",
      "prettier",
      "plugin:prettier/recommended",
      "eslint-config-prettier"
    ],
    "parser": "babel-eslint",
    "rules": {
      "no-unused-vars": "off",
      "react/no-array-index-key": "off",
      "react/forbid-prop-types": "off",
      "import/no-unresolved": "off",
      "import/prefer-default-export": "off",
      "semi": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 100
        }
      ]
    },
    "plugins": ["prettier"]
}
